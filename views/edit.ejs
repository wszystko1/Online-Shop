<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Main Page</title>
    <link rel="stylesheet" href="/css/edit.css">
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600&display=swap" rel="stylesheet">
    <script>
        (async function loadNav() {
            const data = await fetch('/load-nav');
            const navHTML = await data.text();
            document.getElementsByTagName('header')[0].innerHTML = navHTML;
        })();
        
        (async function loadProductList() {
            const data = await fetch('/load-prod-list');
            const productList= await data.text();
            const list = document.getElementsByClassName('result-body')[0];
            list.innerHTML = productList;
        })();

    </script>
</head>
<body>
    <div id="header-wrapper">
        <header>
        </header>
    </div>
        <div class="result-body">

        </div>
    </div>
</body>

